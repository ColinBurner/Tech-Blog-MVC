<h1>Homepage</h1>
<div class="container">
    {{#each posts}}
    <div class="card">
        <h2>{{this.title}}</h2>
        <p>{{this.content}}</p>
        <p>Created by: {{this.user.username}} on {{format_date this.createdAt}}</p>
        <a href="/post/{{this.id}}">Read More</a>
    </div>
    {{/each}}
</div>